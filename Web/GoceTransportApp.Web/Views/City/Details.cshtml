@model GoceTransportApp.Web.ViewModels.Cities.CityDetailsViewModel

@{
    ViewData["Title"] = "City Details";
}

@if (TempData["SuccessMessage"] != null)
{
    <div class="alert alert-success alert-dismissible fade show" role="alert">
        <strong><i class="fas fa-ticket-alt me-2"></i> </strong> @TempData["SuccessMessage"]
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
}

<div class="container mt-5">
    <div class="row justify-content-center">
        <div class="col-md-8">
            <partial name="_CityDetailsPartial" model="Model" />
        </div>
    </div>
</div>