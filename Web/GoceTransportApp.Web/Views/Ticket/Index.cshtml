@model IEnumerable<GoceTransportApp.Web.ViewModels.Tickets.TicketDataViewModel>

@{
    ViewData["Title"] = "Tickets";
}

<h1 class="text-center mt-4"><i class="fas fa-ticket-alt"></i> Tickets</h1>

<div class="row mt-4">
    @foreach (var ticket in Model)
    {
        <partial name="_TicketCardPartial" model="ticket" />
    }
</div>
